{
  "appId": "com.albaseet.admin",
  "appName": "ALBASEET Admin",
  "webDir": "dist",
  "server": {
    "androidScheme": "https",
    "cleartext": true
  },
  "android": {
    "allowMixedContent": true,
    "backgroundColor": "#0A0A0A"
  },
  "plugins": {
    "SplashScreen": {
      "launchShowDuration": 2000,
      "backgroundColor": "#FFD700",
      "androidSplashResourceName": "splash",
      "showSpinner": false
    }
  }
}
